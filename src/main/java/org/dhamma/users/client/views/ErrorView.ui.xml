<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:t="urn:import:org.dhamma.users.client.editors">

  <g:FlowPanel styleName="gwt-rails-display users-errors">

    <g:Label>Errors</g:Label>

    <g:FlowPanel ui:field="model" styleName="gwt-rails-model">

      <t:ErrorEditor ui:field="editor" styleName="gwt-rails-model-editor" />

    </g:FlowPanel>
    
    <g:FlowPanel styleName="gwt-rails-model-list">
      <g:FlexTable ui:field="list"/>
    </g:FlowPanel>

  </g:FlowPanel>

</ui:UiBinder>